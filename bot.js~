function Bot () {
    this.hey = "hey";
    this.pic = new Kinetic.Rect({
    	x: 239,
    	y: 75,
    	width: 100,
    	height: 50,
    	fill: 'green',
    	stroke: 'black',
    	strokeWidth: 4
    });
    layer.add(this.pic);
    layer.draw();
}


console.log("wefhfwehifhwieofhweifohweifowehhfieoh");

Bot.prototype.heya = function() {
    console.log("what is going on");
};

Bot.prototype.move = function() {
    (new Kinetic.Tween({
	node: this.pic,
	x: this.pic.attrs.x + 100,
	duration: 1
    })).play();
};

Bot.prototype.show = function() {
};
